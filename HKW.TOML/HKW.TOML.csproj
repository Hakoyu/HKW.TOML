<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.1.8</Version>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Hakoyu/HKW.TOML</RepositoryUrl>
    <Authors>HKW</Authors>
    <Company>HKW</Company>
    <Description>HKW.TOML is TOML reader, writer, deserializer, serializer and generate classes for C#.</Description>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../README.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="HKW.Utils" Version="*" />
  </ItemGroup>

</Project>
